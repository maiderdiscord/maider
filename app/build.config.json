{
  "appId": "net.maider.maider",
  "productName": "Maider",
  "directories": {
    "output": "dist",
    "buildResources": "resources"
  },
  "files": [
    "**/*.cjs",
    {
      "from": "build",
      "to": ""
    }
  ],
  "extraResources": [
    {
      "from": "golib.dll",
      "to": ".."
    }
  ],
  "win": {
    "target": "nsis",
    "icon": "resources/icon.ico"
  }
}
